<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
                      http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>CatalogServlet</servlet-name>
        <servlet-class>project1.lesson15.servlets.CatalogServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CatalogServlet</servlet-name>
        <url-pattern>/catalog</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ShowCatalogServlet</servlet-name>
        <servlet-class>project1.lesson15.servlets.ShowCatalogServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>ShowCatalogServlet</servlet-name>
        <url-pattern>/showcatalog</url-pattern>
    </servlet-mapping>
</web-app>